{
  "name": "control-api",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=10.8.0",
    "node": ">=22"
  },
  "scripts": {
    "postinstall": "npm run prisma:generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "prisma migrate deploy && NODE_ENV=production npm run start",
    "prisma:clear-db": "rm -fR node_modules/.postgres",
    "preprisma:push-db": "prisma-import --force",
    "prisma:push-db": "prisma db push",
    "preprisma:push-reset-db": "prisma-import --force",
    "prisma:push-reset-db": "prisma db push --force-reset",
    "preprisma:seed-db": "prisma-import --force",
    "prisma:seed-db": "prisma db seed",
    "prisma:studio": "prisma studio",
    "preprisma:generate": "prisma-import --force",
    "prisma:generate": "prisma generate",
    "migrate:reset": "prisma migrate reset",
    "prisma:reset-dev": "DATABASE_PORT=7431 prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "check:typescript": "npx --no-install tsc --noEmit -p tsconfig.json"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "schema": "./prisma/schema.prisma",
    "import": {
      "schemas": "./prisma/models/*.prisma",
      "output": "./prisma/schema.prisma"
    }
  },
  "dependencies": {
    "@originlabs/graphql-postgres-subscriptions-retry": "^3.2.0",
    "@prisma/client": "^5.19.1",
    "@typescript-eslint/parser": "^8.5.0",
    "apollo-server-micro": "^3.13.0",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.6.0",
    "ejs": "^3.1.10",
    "file-type": "^19.4.1",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^16.0.2",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.10",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "micro": "^10.0.1",
    "next": "^12.3.4",
    "nexus": "^1.3.0",
    "node-fetch": "^3.3.2",
    "node-html-parser": "^6.1.13",
    "node-mailjet": "^6.0.6",
    "parse-duration": "^1.1.0",
    "prisma": "^5.19.1",
    "rate-limiter-flexible": "^5.0.3",
    "raw-body": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.25.6",
    "@commitlint/config-conventional": "^19.4.1",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@types/bcrypt": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/eslint__js": "^8.42.3",
    "@types/graphql-upload": "^16.0.7",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/md5": "^2.3.5",
    "@types/node": "22.5.5",
    "@types/node-mailjet": "^3.3.12",
    "@types/react": "18.3.5",
    "@types/sharp": "^0.32.0",
    "commitlint": "^19.4.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^7.0.2",
    "eslint-plugin-import": "^2.30.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma-import": "^1.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.4.0"
  }
}
